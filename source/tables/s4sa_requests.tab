create table S4SA_REQUESTS
(
  id              INTEGER not null,
  tijd            TIMESTAMP(6) default systimestamp not null,
  response        CLOB,
  request_source  VARCHAR2(50) not null,
  request_uri     VARCHAR2(1000) not null,
  request_type    VARCHAR2(100) not null,
  request_headers VARCHAR2(2000),
  request_body    CLOB,
  application     INTEGER,
  gebruiker       VARCHAR2(255)
)
;

